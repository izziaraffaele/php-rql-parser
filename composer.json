{
    "name": "graviton/php-rql-parser",
    "homepage": "https://github.com/libgraviton/php-rql-parser",
    "description": "A standalone RQL (Resource Query Language) Parser with flexible usage possibilities",
    "keywords": ["rest", "rql", "resource", "query", "language", "parser"],
    "license": "GPL",
    "authors": [
        {
            "name": "Dario Nuevo",
            "email": "dario.nuevo@swisscom.com"
        }
    ],
    "autoload": {
        "psr-4": { "Graviton\\Rql\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": { "Graviton\\Rql\\": "test/"}
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "~2.2",
        "doctrine/mongodb-odm": "1.0.0-BETA11@dev",
        "doctrine/data-fixtures": "~1.0"
    },
    "require": {
        "php": "~5.4",
        "doctrine/lexer": "~1.0",
        "socalnick/orchestrate-php-client": "0.5.*"
    },
    "suggest": {
        "doctrine/mongodb-odm": "Needs ~1.0 if you want to use Graviton\\Rql\\Queriable\\MongoOdm."
    },
    "minimum-stability": "stable"
}
